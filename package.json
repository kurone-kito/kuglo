    "tsconfig-paths": "^3.8.0",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "~3.3.4000",
    "vinyl": "^2.2.0",
    "webpack": "^4.29.6",
    "webpack-stream": "^5.2.1"
  },
  "husky": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{css,js,json,md,scss,ts,tsx,yml}": [
      "prettier --write",
      "git add"
    ]
  }
