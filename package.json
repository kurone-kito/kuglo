    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tsconfig-paths-jest": "0.0.1",
    "typescript": "^3.5.3",
    "vinyl": "^2.2.0",
    "webpack": "^4.39.2",
    "webpack-stream": "^5.2.1"
  },
  "husky": {
    "pre-commit": "lint-staged"
  },
