{
  "name": "kuglo",
  "version": "0.1.0",
  "description": "Practice for desktop app using Electron",
  "author": "kurone-kito",
  "license": "CC-BY-NC-4.0",
  "keywords": [
    "electron",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kurone-kito/kuglo.git"
  },
  "bugs": {
    "url": "https://github.com/kurone-kito/kuglo/issues"
  },
  "homepage": "https://github.com/kurone-kito/kuglo#readme",
  "main": "main.js",
  "scripts": {
    "start": "gulp",
    "test": "gulp test",
    "build:content:dev": "gulp build:content --mode development",
    "build:content:prod": "gulp build:content --mode production",
    "build:binary": "gulp build:binary:full --windows --linux",
    "build:binary:mac": "npm run build:binary -- --macos",
    "build:production": "npm run build:binary -- --mode production",
    "build:production:mac": "npm run build:binary:mac -- --mode production",
    "build:storybook": "gulp build:sb:rebuild",
    "start:storybook": "gulp run:sb",
    "test:coverage": "gulp test:coverage",
    "test:visual": "gulp run:backstop:rebuild --backstop test",
    "test:visual:approve": "gulp run:backstop:approve",
    "test:visual:report": "gulp run:backstop:report",
    "test:visual:spec": "gulp run:backstop:rebuild --backstop reference"
  },
  "engines": {
    "node": ">=12.6"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": "^4.3.2"
  },
