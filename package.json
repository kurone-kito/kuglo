{
  "name": "kuglo",
  "version": "0.1.0",
  "description": "Practice for desktop app using Electron",
  "author": "kurone-kito",
  "license": "CC-BY-NC-4.0",
  "keywords": [
    "electron",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kurone-kito/kuglo.git"
  },
  "bugs": {
    "url": "https://github.com/kurone-kito/kuglo/issues"
  },
  "homepage": "https://github.com/kurone-kito/kuglo#readme",
  "main": "main.js",
  "scripts": {
    "install": "gulp build:content",
    "start": "gulp",
    "test": "gulp test",
    "build:content:dev": "gulp build:content --mode development",
    "build:content:prod": "gulp build:content --mode production",
    "build:binary": "gulp build:binary --windows --linux",
    "build:binary:mac": "npm run build:binary -- --macos",
    "clean": "gulp clean",
    "production": "npm run build:binary -- --mode production",
    "production:mac": "npm run build:binary:mac -- --mode production",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "storybook": "gulp storybook:start",
    "storybook:toc": "gulp storybook:toc",
    "storybook:build": "gulp storybook:rebuild",
    "visual:spec": "backstop reference --configPath=backstop.config.js",
    "visual:test": "backstop test --configPath=backstop.config.js",
    "visual:approve": "backstop approve --configPath=backstop.config.js"
  },
  "engines": {
    "node": ">=8.10"
  },
  "dependencies": {
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@storybook/react": "^5.0.0",
    "@types/babel__core": "^7.1.0",
    "@types/enzyme": "^3.1.18",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/eslint": "^4.16.6",
    "@types/gulp": "^4.0.6",
    "@types/jest": "^24.0.11",
    "@types/minimist": "^1.2.0",
    "@types/node-fetch": "^2.1.6",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "@types/rmfr": "^2.0.0",
    "@types/storybook__react": "^4.0.1",
    "@types/styled-components": "^4.1.12",
    "@types/through2": "^2.0.34",
    "@types/vinyl": "^2.0.2",
    "@types/webpack": "^4.4.25",
    "@types/webpack-stream": "^3.2.10",
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "babel-loader": "^8.0.5",
    "backstopjs": "^3.9.4",
    "coveralls": "^3.0.3",
    "electron": "^4.0.6",
    "electron-builder": "^20.38.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "gulp": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "minimist": "^1.2.0",
    "node-fetch": "^2.3.0",
    "prettier": "^1.16.4",
    "query-string": "^6.3.0",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "rmfr": "^2.0.0",
    "sync-request": "^6.0.0",
    "through2": "^3.0.1",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tsconfig-paths-jest": "0.0.1",
    "typescript": "^3.3.3333",
    "vinyl": "^2.2.0",
    "webpack": "^4.29.6",
    "webpack-stream": "^5.2.1"
  },
  "husky": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{css,js,json,md,scss,ts,tsx,yml}": [
      "prettier --write",
      "git add"
    ]
  }
}
